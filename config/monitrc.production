check process delayed_job with pidfile /var/www/pluie/tmp/pids/delayed_job.pid
start program = "/bin/su - pepe -c 'cd /var/www/pluie/; bin/delayed_job --queues=deliver,bulksms start >> log/delayed_job.log 2>&1'"
stop program = "/bin/su - pepe -c 'cd /var/www/pluie/; bin/delayed_job --queues=deliver,bulksms stop >> log/delayed_job.log 2>&1'"
