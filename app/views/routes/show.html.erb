<p>
  <strong><%= Route.human_attribute_name("name") %>:</strong>
  <%= @route.name %>
</p>

<p>
  <strong><%= Route.human_attribute_name("price") %>:</strong>
  <%= @route.price %>
</p>

<p>
  <strong><%= Route.human_attribute_name("system_route") %>:</strong>
  <%= @route.system_route %>
</p>

<p>
