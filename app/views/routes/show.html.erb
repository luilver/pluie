  <ul>
  <%@route.users.each do |u|%>

  <li><%= u.username%></li>
  <%end%>
  </ul>
  </p>

<p>
  <strong><%= Gateway.model_name.human %>:</strong>
  <%= @route.gateway %>
</p>

